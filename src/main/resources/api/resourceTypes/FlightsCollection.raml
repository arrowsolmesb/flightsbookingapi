#%RAML 1.0 ResourceType
get:
  description: Retrieve list of <<resourcePathName>>
  queryParameters:
      source:
        example: BLR
        required: false
      destination:
        example: DEL
      doj:
        example: 30-Sep-2022
  responses: 
    200:
      body:
        application/json:
         type: <<resourcePathName | !singularize | !uppercamelcase>>
         